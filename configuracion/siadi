);

create table justificacion(
id serial primary key,
idtipojustificacion int not null,
fechainicio date not null,
fechafin date not null,
horainicio time not null,
horafin time null,
descripcion varchar(250) null
);

create table permiso(
id serial primary key,
idcontrato int not null,
idttipopermiso int not null,
fecha date not null,
horasalida timestamp not null,
horaretorno timestamp null,
autorizja smallint not null,
autorizjp smallint not null,
destino varchar(250) null,
descripcion varchar(250) null,
constraint fk_permcon foreign key(idcontrato) references contrato(id)
);

create table sancion(
id serial primary key,
idtiposancion int not null,
fechainicio date not null,
fechafin date not null,
descripcion varchar(250) null
);

create table control(
id serial primary key,
idcontrato int not null,
idpermiso int null,
idjustificacion int null,
idsancion int null,
fecha date not null,
idusuariocrea int not null,
fechusuariocrea date not null,
idusuariomod int null,
fechusuariomod date null,
constraint fk_contcon foreign key(idcontrato) references contrato(id),
constraint fk_contper foreign key(idpermiso) references permiso(id),
constraint fk_contjus foreign key(idjustificacion) references justificacion(id),
constraint fk_contsan foreign key(idsancion) references sancion(id)
);

