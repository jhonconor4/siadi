-------------------------------MODULO PERSONAL------------------------------------ 
create table agencia(
id serial primary key not null,
iddistrito int null,
razonsocial varchar(200) not null,
nombrecomercial varchar(200) not null,
ruc numeric(11,0) not null,
telefono numeric(11,0) not null,
urllogo varchar(300) not null,
direccion varchar(100) not null
);

insert into agencia(iddistrito,razonsocial,nombrecomercial,ruc,telefono,urllogo,direccion) values(1,'BESOFT TECNOLOGIES S.A.C','BESOFT',20478878805,929181954,'','JR. SAN MARTIN 225');

create table modulo(
id serial primary key,
nombre varchar(100) not null,
codigo varchar(10) not null,
ruta varchar(100) not null,
icono varchar(25) not null,
estado boolean not null
);

INSERT INTO modulo VALUES (1, 'Modulo almacen', '01', '/almacen', 'fa-cubes', true);
INSERT INTO modulo VALUES (2, 'Modulo venta', '02', '/pos', 'fa-shopping-cart', true);
INSERT INTO modulo VALUES (3, 'Modulo personal', '03', '/personal', 'fa-users', true);

create table rol(
id serial primary key,
idmodulo int not null,
nombre varchar(100) not null,
codigo varchar(10) not null,
ruta varchar(100) not null,
icono varchar(25) not null,
estado boolean not null,
constraint fk_rolmodul foreign key (idmodulo) references modulo(id)
);

INSERT INTO rol VALUES (1, 1, 'Administracion', '01', '/administracion', 'fa-desktop', true);
