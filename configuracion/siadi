create table colaborador(
id serial primary key,
idagencia int not null,
iddistrito int not null,
idtestadocivil int not null,
idtgenero int not null,
dni numeric(8,0) not null,
nombre varchar(50) not null,
apellidopat varchar(50) not null,
apellidomat varchar(50) not null,
celular numeric(11,0) not null,
cargo varchar(50) not null,
direccion varchar(100) not null,
email varchar(50) null,
ruc numeric(11,0) null,
fechanac date null,
huella bytea null,
constraint fk_colabagencia foreign key (idagencia) references agencia(id)
);

create table usuario(
id serial primary key not null,
idcolaborador int not null,
usuario varchar(50) not null,
contrasena varchar(50) not null,
estado boolean not null,
idusuariocrea int not null,
fechusuariocrea date not null,
idusuariomod int null,
fechusuariomod date null,
constraint fk_usuarcolab foreign key (idcolaborador) references colaborador(id)
);

create table moduloacceso(
id serial primary key,
idusuario int not null,
nombre varchar(100) not null,
codigo varchar(10) not null,
ruta varchar(100) not null,
icono varchar(25) not null,
estado boolean not null,
constraint fk_modaccusuar foreign key (idusuario) references usuario(id)
);

create table rolacceso(
id serial primary key,
idmoduloacceso int not null,
nombre varchar(100) not null,
codigo varchar(10) not null,
ruta varchar(100) not null,
icono varchar(25) not null,
estado boolean not null,
constraint fk_rolaccmodulacc foreign key (idmoduloacceso) references moduloacceso(id)
);

create table funcionacceso(
id serial primary key,
