fechafin date not null,
descripcion varchar(250) null,
idusuariocrea int not null,
fechusuariocrea date not null,
idusuariomod int null,
fechusuariomod date null,
constraint fk_vaccon foreign key(idcontrato) references contrato(id)
);

create table turno(
id serial primary key,
idcontrato int not null,
idttipoturno int not null,
dia int not null,
horaentrada timestamp not null,
horasalida timestamp not null,
constraint fk_turcon foreign key(idcontrato) references contrato(id)
);

create table asistencia(
id serial primary key,
idcontrato int not null,
idturno int not null,
idttipoasistencia int not null,
tardanza int not null,
fecha date not null,
horaentrada timestamp not null,
